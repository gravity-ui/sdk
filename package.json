{
  "name": "@gravity-ui/sdk",
  "version": "1.6.1-alpha.0",
  "description": "",
  "homepage": "https://github.com/gravity-ui/sdk#readme",
  "bugs": {
    "url": "https://github.com/gravity-ui/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravity-ui/sdk.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": {
        "types": "./build/cjs/index.d.ts",
        "default": "./build/cjs/index.js"
      }
    }
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp",
    "lint": "eslint \"lib/**/*.{js,jsx,ts,tsx}\" --quiet",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@gravity-ui/axios-wrapper": "^1.5.1",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@gravity-ui/eslint-config": "^2.2.0",
    "@gravity-ui/gulp-utils": "^1.0.3",
    "@gravity-ui/prettier-config": "^1.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@swc/core": "^1.11.24",
    "@swc/jest": "^0.2.38",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.0",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.0",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nano-staged": "^0.6.0",
    "nock": "^13.2.6",
    "prettier": "^2.8.6",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@gravity-ui/gateway": ">=1.0.4"
  }
